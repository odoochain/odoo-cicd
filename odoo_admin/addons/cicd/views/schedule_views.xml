<odoo>
	<data noupdate='0'>
		<record id="view_cicd_schedule_form" model="ir.ui.view">
			<field name="name">cicd.schedule form</field>
			<field name="model">cicd.schedule</field>
			<field name="arch" type="xml">
				<form string="">
					<header></header>
					<sheet>
						<group>
							<field name="name"/>
							<field name="schedule"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="view_cicd_schedule_tree" model="ir.ui.view">
			<field name="name">cicd.schedule tree</field>
			<field name="model">cicd.schedule</field>
			<field name="arch" type="xml">
				<tree string="Schedules" editable="bottom">
					<field name="name"/>
					<field name="schedule"/>
				</tree>
			</field>
		</record>

	</data>
</odoo>
